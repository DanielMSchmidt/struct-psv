  color: #FFFFFF;
}

#maincontent #pricing #tabelle td{
  vertical-align: middle;
}

#maincontent #pricing #tabelle th{
  vertical-align: middle;
}

#maincontent #pricing #boxes .checkbox{
  float: left;
  width: 200px;
  height: 75px;
}

#maincontent #pricing #boxes .inputvalue{
  float: right;
  float: left;
  width: 175px;
  height: 75px;
}

